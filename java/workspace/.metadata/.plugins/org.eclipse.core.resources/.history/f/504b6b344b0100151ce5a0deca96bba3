public class ListCellData {

	public ListCellData(Context context,String controlName, Intent targetIntent) {
		// TODO Auto-generated constructor stub
	}
	
	public Context getContext() {
		return context;
	}
	public void setContext(Context context) {
		this.context = context;
	}
	public String getContraString() {
		return contraString;
	}
	public void setContraString(String contraString) {
		this.contraString = contraString;
	}
	public Intent getTargIntent() {
		return targIntent;
	}
	public void setTargIntent(Intent targIntent) {
		this.targIntent = targIntent;
	}

	private Context context;
	private String contraString;
	private Intent targetIntent;
}
