	@Override
	public void onCreate(Bundle param){
		super.onCreate(param);
		setContentView(R.layout.my_layout);
		
		DatePicker date = (DatePicker) findViewById(R.id.datepicker);
		TimePicker time = (TimePicker) findViewById(R.id.timepicker);
		
	}
}
